{
    "manifest_version": 2,
    "name": "WPI BannerWeb to CSV",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://bannerweb.wpi.edu/pls/prod/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": [
            "imports.js",
            "calendar_page.js"
        ],
        "persistent": false
    },
    "web_accessible_resources": [
        "course.js",
        "schedule_changes.js"
    ],
    "permissions": [
        "https://bannerweb.wpi.edu/pls/prod/*",
        "https://www.wpi.edu/offices/registrar/calendar"
    ],
    "icons": {
        "128": "128.png"
    }
}
