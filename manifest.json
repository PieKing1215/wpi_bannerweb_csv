{
    "manifest_version": 2,
    "name": "WPI BannerWeb to CSV",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://bannerweb.wpi.edu/pls/prod/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["imports.js", "get_calendar_page.js"],
        "persistent": false
    },
    "web_accessible_resources": ["lib/*"],
    "permissions": [
        "https://bannerweb.wpi.edu/pls/prod/*",
        "https://www.wpi.edu/offices/registrar/calendar"
    ],
    "icons": {
        "128": "128.png"
    }
}
