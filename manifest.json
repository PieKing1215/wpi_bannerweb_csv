{
    "manifest_version": 2,
    "name": "WPI BannerWeb to CSV",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://bannerweb.wpi.edu/pls/prod/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["https://bannerweb.wpi.edu/pls/prod/*"],
    "icons": {
        "128": "128.png"
    }
}
